<!--index.wxml-->
<view class="container">
 <map
  id="map"
  longitude="{{longitude}}"
  latitude="{{latitude}}"
  scale="14"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
  
></map>
</view>
<view id='tabbar'>
<view bindtap='handlePublic'>发布</view>
<view bindtap='handleSearch'>搜索</view>
</view>
