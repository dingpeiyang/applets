.container{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 100rpx;
  width: 100%;
}
#map{
  width: 100%;
  height: 100%;
}
#tabbar{
  display: flex;
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  justify-content: space-around;
  color:white;
}
#tabbar>view{
  text-align: center;
  line-height: 100rpx;
  width: 100%;
  height: 100rpx;
}
#tabbar>view:nth-child(1){
  background: #ff9700;
}
#tabbar>view:nth-child(2){
  background: #03bbd5;
}